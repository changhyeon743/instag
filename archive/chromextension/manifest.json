{
	"name": "INSTAG",
	"description": "A website which can recommend you some Instagram tags by typing subject you want",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
		"default_icon": {
			"16": "icons/icon-16.png",
			"48": "icons/icon-48.png",
			"128": "icons/icon-128.png"
		},
		"default_popup": "index.html"
	},
	"permissions": [
		"activeTab",
		"http://*/*",
		"https://*/*"
	]
}